# Configuration for Karpathy Agent
# Copy this file to .env and fill in your configuration

# Nexus LiteLLM Proxy Configuration
# URL to your LiteLLM proxy server
NEXUS_URL=https://nexus-master.lmndstaging.com

# API key for the LiteLLM proxy
LITELLM_PROXY_API_KEY='sk-12345'

# Model to use (in LiteLLM format)
# Examples: anthropic/claude-opus-4-5-20251101, openai/gpt-5-2025-08-07
AGENT_MODEL=anthropic/claude-opus-4-5-20251101
